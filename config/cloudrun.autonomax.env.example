ENVIRONMENT=production
DEBUG=false
HOST=0.0.0.0
APP_TARGET=autonomax

# Core (provided via Secret Manager)
# SECRET_KEY -> autonomax-secret-key
# DATABASE_URL -> autonomax-database-url

# Cloud SQL (optional)
# CLOUD_SQL_INSTANCE=project:region:instance
# DATABASE_URL=postgresql+asyncpg://USER:PASSWORD@/DBNAME?host=/cloudsql/project:region:instance

# CORS
SECURITY_CORS_ORIGINS=["https://REPLACE_WITH_AUTONOMAX_UI_URL"]
FRONTEND_ORIGIN=https://REPLACE_WITH_AUTONOMAX_UI_URL
BACKEND_ORIGIN=https://REPLACE_WITH_AUTONOMAX_API_URL

# Shopier app mode
SHOPIER_APP_MODE=true
