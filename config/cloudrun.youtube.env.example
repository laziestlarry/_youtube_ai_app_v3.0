ENVIRONMENT=production
DEBUG=false
HOST=0.0.0.0
APP_TARGET=youtube

# Core (provided via Secret Manager)
# SECRET_KEY -> youtube-ai-secret-key
# DATABASE_URL -> youtube-ai-database-url

# Cloud SQL (optional)
# CLOUD_SQL_INSTANCE=project:region:instance
# DATABASE_URL=postgresql+asyncpg://USER:PASSWORD@/DBNAME?host=/cloudsql/project:region:instance

# CORS
SECURITY_CORS_ORIGINS=["https://REPLACE_WITH_YOUTUBE_UI_URL"]
FRONTEND_ORIGIN=https://REPLACE_WITH_YOUTUBE_UI_URL
BACKEND_ORIGIN=https://REPLACE_WITH_YOUTUBE_API_URL

# Shopier app mode
SHOPIER_APP_MODE=true
